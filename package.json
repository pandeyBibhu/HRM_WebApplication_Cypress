{
  "name": "my-cypress-project",
  "version": "1.0.0",
  "description": "A sample Cypress project for testing.",
  "main": "index.js",
  "scripts": {
    "test": "cypress open",
    "test:run": "cypress run",
   "report:merge": "mochawesome-merge cypress/reports/mocha/*.json > cypress/reports/mochawesome.json",
  "report:generate": "marge cypress/reports/mochawesome.json --reportDir cypress/reports/html",
  "report": "npm run test && npm run report:merge && npm run report:generate"
  },
  "devDependencies": {
    "cypress": "^15.1.0",
    "cypress-multi-reporters": "^2.0.5",
    "cypress-xpath": "^2.0.1",
    "mocha": "^11.7.2",
    "mocha-junit-reporter": "^2.2.1",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^5.0.0",
    "mochawesome-report-generator": "^6.2.0"
  },
  "author": "",
  "license": "MIT"
}
